{
  "name": "cms_admin",
  "version": "0.0.1",
  "description": "admit ui for cms",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr --port 1420",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "tauri": "tauri",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.9.1",
    "@builder.io/qwik-city": "^1.9.1",
    "@modular-forms/qwik": "^0.29.0",
    "@qwik-ui/headless": "^0.6.2",
    "@qwikest/icons": "^0.0.13",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/builder-vite": "^8.2.8",
    "@storybook/html": "^8.2.8",
    "@storybook/html-vite": "^8.2.8",
    "@tauri-apps/api": "^2",
    "@tauri-apps/cli": "^2",
    "@types/eslint": "8.56.10",
    "@types/node": "20.14.11",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@unocss/preset-mini": "^0.56.5",
    "@unocss/reset": "^0.56.5",
    "@unocss/transformer-variant-group": "^0.57.1",
    "@unocss/vite": "^0.63.4",
    "animated-unocss": "^0.0.6",
    "autoprefixer": "^10.4.19",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "^1.9.1",
    "globals": "^15.11.0",
    "postcss": "^8.4.39",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "storybook": "^8.2.8",
    "storybook-dark-mode": "^4.0.2",
    "storybook-framework-qwik": "^0.4.0",
    "tailwindcss-preflight": "^1.0.1",
    "typescript": "5.4.5",
    "typescript-eslint": "^8.10.0",
    "undici": "*",
    "unocss": "^0.63.4",
    "valibot": "1.0.0-beta.8",
    "vite": "5.3.5",
    "vite-tsconfig-paths": "^4.2.1",
    "flowbite-qwik": "^0.37.10"
  }
}
